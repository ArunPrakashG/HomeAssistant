  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
  </PropertyGroup>

  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="xcopy &quot;$(SolutionDir)Youtube\bin\Debug\netcoreapp2.1\Youtube.dll&quot; &quot;$(SolutionDir)HomeAssistant\Modules\YoutubeModules&quot; /Y /I" />
  </Target>

  <ItemGroup>
    <ProjectReference Include="..\HomeAssistant\Assistant.csproj" />
  </ItemGroup>

</Project>
